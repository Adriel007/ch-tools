<?xml version="1.0"?>
<yandex>
    <profiles>
        <default>
            <distributed_ddl_task_timeout>5</distributed_ddl_task_timeout>
            <max_memory_usage>10000000000</max_memory_usage>
            <use_uncompressed_cache>0</use_uncompressed_cache>
        </default>
    </profiles>

    <users>
        <default>
            <password></password>
            <networks incl="networks" replace="replace">
                <ip>::/0</ip>
            </networks>
            <profile>default</profile>
            <quota>default</quota>
        </default>
    </users>

    <quotas>
        <default>
        </default>
    </quotas>
</yandex>